<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
<!-- HandlerMapping -->
<!-- <bean id="defaultHandlerMapping"
	class="org.springframework.web.servlet.handler.BeanNameUrlHandlerMapping"/>
 --> 
 
<!-- Controller-->	
<bean class="spring.mybatis.controller.ListController">
	<property name="dao" ref="memProcessDao"/>
</bean>
<bean class="spring.mybatis.controller.InsertController">
	<property name="dao" ref="memProcessDao"/>
</bean>
<bean class="spring.mybatis.controller.UpdateController">
	<property name="dao" ref="memProcessDao"/>
</bean>
<bean class="spring.mybatis.controller.DeleteController">
	<property name="dao" ref="memProcessDao"/>
</bean>
<!-- ViewResolver -->
<bean id="viewResolver"
	class="org.springframework.web.servlet.view.InternalResourceViewResolver">
	<property name="prefix" value="/"/>
	<property name="suffix" value=".jsp"/><!-- /list1.jsp -->
</bean>

</beans>
